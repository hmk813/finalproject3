<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="staffImg">
	
	<!-- 사원 사진 등록 -->
	<insert id="insert" parameterType="StaffImgDto">
		insert into staffImg(imgAttachNo, imgStaffId)
		values(#{imgAttachNo}, #{imgStaffId})
	</insert>	
	
	<!-- 사원 사진 목록  -->
	<select id="list" resultType="StaffImgDto">
		select * from staffImg
	</select>
</mapper>